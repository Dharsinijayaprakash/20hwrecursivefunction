#include<stdio.h>
int main()
{
    int u,f=0,g=1,res;
    printf("Enter the number : ");
    scanf("%d",&u);
    printf("Fibonacci series is :");
    for(int i=0;i<=u-2;i++)
    {
        printf("%d ",fib(i));
    }
}
int fib(int u)
{
    if(u<=1)
    {
        return u;
    }
    else
    {
       return fib(u-1)+fib(u-2);
    }
}
